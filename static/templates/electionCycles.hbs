  {{#each this}}
  <label>
    <input
        type="radio"
        name="cycle-toggle"
        value="{{max}}"
        data-period="{{period}}"
        {{#if checked}}checked{{/if}}
      />
    <span class="button--neutral">
      {{#if period}}
      Full cycle:
      {{/if}}
      {{min}}â€“{{max}}
    </span>
  </label>
  {{/each}}
</fieldset>
