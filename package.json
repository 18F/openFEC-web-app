    "glossary-panel": "1.0.0",
    "handlebars": "^4.0.5",
    "hbsfy": "2.2.1",
    "intl": "1.0.0-rc-4",
    "jquery": "2.2.4",
    "jquery.inputmask": "3.2.7",
    "keyboardjs": "0.4.2",
    "leaflet": "0.7.7",
